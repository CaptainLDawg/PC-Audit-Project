<Window x:Class="PCAudit.ExportingInformation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ExportingInformation" Height="175" Width="356" Background="#FF9C9892" Loaded="Window_Loaded" >
    <Grid>
        <Label x:Name="lblLoading" Content="Your information is currently being exported" HorizontalAlignment="Left" Margin="27,32,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="311" Background="{x:Null}" FontSize="16" FontFamily="Gill Sans MT" RenderTransformOrigin="0.505,0"/>
        <Label x:Name="lblTimer" Content="Please Wait.." HorizontalAlignment="Left" Margin="124,78,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="113" Background="{x:Null}" FontSize="16" FontFamily="Gill Sans MT" RenderTransformOrigin="0.505,0"/>
    </Grid>
</Window>
