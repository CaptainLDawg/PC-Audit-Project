<Window x:Class="PCAudit.LoginToChangeControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ExportingInformation" Height="138.216" Width="305.32" Background="#FF9C9892" >
    <Grid>
        <Label x:Name="lblUser" Content="Enter Username: " HorizontalAlignment="Left" Margin="13,14,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="124" Background="{x:Null}" FontSize="16" FontFamily="Gill Sans MT" RenderTransformOrigin="0.505,0"/>
        <Label x:Name="lblPass" Content="Enter Password " HorizontalAlignment="Left" Margin="13,43,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="124" Background="{x:Null}" FontSize="16" FontFamily="Gill Sans MT" RenderTransformOrigin="0.505,0"/>
        <TextBox x:Name="txtUser" HorizontalAlignment="Left" Height="23" Margin="142,17,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="139"/>
        <TextBox x:Name="txtPass" HorizontalAlignment="Left" Height="23" Margin="142,45,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="139"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="142,77,0,0" VerticalAlignment="Top" Width="139" Click="btnLogin_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="18,77,0,0" Width="115" Click="btnCancel_Click" Height="20" VerticalAlignment="Top"/>
    </Grid>
</Window>